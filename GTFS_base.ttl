@prefix gtfs: <http://gtfs.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .

gtfs:ConditionallyForbidden a owl:Class ;
    rdfs:label "ConditionallyForbidden"@en ;
    rdfs:comment "The field or file must not be included under conditions outlined in the field or file description." ;
    rdfs:subClassOf gtfs:PresenceCondition .

gtfs:CurrencyAmount a owl:Class ;
    rdfs:label "CurrencyAmount"@en ;
    rdfs:comment "A decimal value indicating a currency amount. The number of decimal places is specified by ISO 4217 for the accompanying Currency code. All financial calculations should be processed as decimal, currency, or another equivalent type suitable for financial calculations depending on the programming language used to consume data. Processing currency amounts as float is discouraged due to gains or losses of money during calculations."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Date a owl:Class ;
    rdfs:label "Date"@en ;
    rdfs:comment "Service day in the YYYYMMDD format. Since time within a service day may be above 24:00:00, a service day may contain information for the subsequent day(s)."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Enum a owl:Class ;
    rdfs:label "Enum"@en ;
    rdfs:comment "An option from a set of predefined constants defined in the \"Description\" column."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:FareProdcut a owl:Class ;
    rdfs:label "FareProdcut"@en ;
    rdfs:comment "Purchasable fare products that can be used to pay for or validate travel." ;
    rdfs:subClassOf schema1:Intangible .

gtfs:Float a owl:Class ;
    rdfs:label "Float"@en ;
    rdfs:comment "A floating point number."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Integer a owl:Class ;
    rdfs:label "Integer"@en ;
    rdfs:comment "An integer" ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Latitude a owl:Class ;
    rdfs:label "Latitude"@en ;
    rdfs:comment "WGS84 latitude in decimal degrees. The value must be greater than or equal to -90.0 and less than or equal to 90.0." ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Leg a owl:Class ;
    rdfs:label "Leg"@en ;
    rdfs:comment "Travel in which a rider boards and alights between a pair of subsequent locations along a trip." ;
    rdfs:subClassOf schema1:Intangible .

gtfs:Longitude a owl:Class ;
    rdfs:label "Longitude"@en ;
    rdfs:comment "WGS84 longitude in decimal degrees. The value must be greater than or equal to -90.0 and less than or equal to 90.0." ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Recommended a owl:Class ;
    rdfs:label "Recommended"@en ;
    rdfs:comment "The field or file may be omitted from the dataset, but it is a best practice to include it. Before omitting this field or file, the best practice should be carefully evaluated and the full implications of omission should be understood." ;
    rdfs:subClassOf gtfs:PresenceCondition .

gtfs:ServiceDay a owl:Class ;
    rdfs:label "ServiceDay"@en ;
    rdfs:comment "A service day is a time period used to indicate route scheduling. The exact definition of service day varies from agency to agency but service days often do not correspond with calendar days. A service day may exceed 24:00:00 if service begins on one day and ends on a following day. For example, service that runs from 08:00:00 on Friday to 02:00:00 on Saturday, could be denoted as running from 08:00:00 to 26:00:00 on a single service day." ;
    rdfs:subClassOf schema1:Intangible .

gtfs:SubJourney a owl:Class ;
    rdfs:label "SubJourney"@en ;
    rdfs:comment "Two or more legs that comprise a subset of a journey" ;
    rdfs:subClassOf gtfs:Journey .

gtfs:Time a owl:Class ;
    rdfs:label "Time"@en ;
    rdfs:comment "Time in the HH:MM:SS format (H:MM:SS is also accepted). The time is measured from \"noon minus 12h\" of the service day (effectively midnight except for days on which daylight savings time changes occur). For times occurring after midnight on the service day, enter the time as a value greater than 24:00:00 in HH:MM:SS." ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:additionalInfo a owl:AnnotationProperty ;
    rdfs:label "additionalInfo"@en ;
    rdfs:comment "Any additional information about the object" .

gtfs:agency a gtfs:File ;
    rdfs:label "agency" ;
    gtfs:hasField gtfs:agency_email,
        gtfs:agency_fare_url,
        gtfs:agency_id,
        gtfs:agency_lang,
        gtfs:agency_name,
        gtfs:agency_phone,
        gtfs:agency_timezone,
        gtfs:agency_url ;
    gtfs:isPresence gtfs:Required ;
    rdfs:comment "Contains all information about transit agencies." .

gtfs:example a owl:AnnotationProperty ;
    rdfs:label "example"@en ;
    rdfs:comment "An example FieldType or use case" .

gtfs:hasField a owl:ObjectProperty ;
    rdfs:label "hasField"@en ;
    rdfs:comment "This property links a file to its fields" ;
    rdfs:domain gtfs:File ;
    rdfs:range gtfs:Field ;
    rdfs:subPropertyOf owl:topObjectProperty .

gtfs:hasPresence a owl:ObjectProperty ;
    rdfs:label "hasPresence"@en ;
    rdfs:comment "This property links a field to its PresenceCondition." ;
    rdfs:domain gtfs:Field,
        gtfs:File ;
    rdfs:range gtfs:PresenceCondition ;
    rdfs:subPropertyOf owl:topObjectProperty .

gtfs:isPresence a owl:AnnotationProperty ;
    rdfs:label "isPresence"@en ;
    rdfs:domain gtfs:Field,
        gtfs:FieldType ;
    rdfs:range gtfs:PresenceCondition .

gtfs:isType a owl:AnnotationProperty,
        owl:DatatypeProperty ;
    rdfs:label "isType",
        "isType"@en ;
    rdfs:domain gtfs:Field,
        <http://www.example.org/gtfs#Field> ;
    rdfs:range gtfs:FieldType .

gtfs:route_type a gtfs:Field ;
    rdfs:label "route_type" ;
    gtfs:additionalInfo "Each route must have a single, non-empty value for this field." ;
    gtfs:example "0" ;
    gtfs:hasEnumValue <http://gtfs.org/route_type/0>,
        <http://gtfs.org/route_type/1>,
        <http://gtfs.org/route_type/2>,
        <http://gtfs.org/route_type/3>,
        <http://gtfs.org/route_type/4>,
        <http://gtfs.org/route_type/5>,
        <http://gtfs.org/route_type/6>,
        <http://gtfs.org/route_type/7> ;
    gtfs:isPresence "Required" ;
    gtfs:isType "Enum" ;
    rdfs:comment "Describes the type of transportation used on a route." .

schema1:Property a owl:Class ;
    rdfs:label "Property"@en .

schema1:QuantitativeValue a owl:Class ;
    rdfs:label "QuantitativeValue"@en .

schema1:StructuredValue a owl:Class ;
    rdfs:label "StructuredValue"@en .

schema1:Vehicle a owl:Class ;
    rdfs:label "Vehicle"@en .

<http://www.example.org/gtfs#hasEnumValue> a rdf:Property ;
    rdfs:label "hasEnumValue"@en ;
    rdfs:comment "This property links a field to its possible enumeration values." ;
    rdfs:domain <http://www.example.org/gtfs#Field> ;
    rdfs:range rdfs:Resource ;
    rdfs:subPropertyOf owl:topObjectProperty .

<urn:webprotege:ontology:a20e8f42-14c4-441d-8ba5-724fed2311a8> a owl:Ontology .

gtfs:ConditionallyRequired a owl:Class ;
    rdfs:label "ConditionallyRequired"@en ;
    rdfs:comment "The field or file must be included under conditions outlined in the field or file description." ;
    rdfs:subClassOf gtfs:PresenceCondition .

gtfs:Email a owl:Class ;
    rdfs:label "Email"@en ;
    rdfs:comment "An email address."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:ID a owl:Class ;
    rdfs:label "ID"@en ;
    rdfs:comment "An ID field value is an internal ID, not intended to be shown to riders, and is a sequence of any UTF-8 characters. Using only printable ASCII characters is recommended. An ID is labeled \"unique ID\" when it must be unique within a file. IDs defined in one .txt file are often referenced in another .txt file. IDs that reference an ID in another table are labeled \"foreign ID\"."@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Journey a owl:Class ;
    rdfs:label "Journey"@en ;
    rdfs:comment "Overall travel from origin to destination, including all legs and transfers in-between" ;
    rdfs:subClassOf schema1:Intangible .

gtfs:LanguageCode a owl:Class ;
    rdfs:label "LanguageCode"@en ;
    rdfs:comment "An IETF BCP 47 language code. For an introduction to IETF BCP 47, refer to http://www.rfc-editor.org/rfc/bcp/bcp47.txt and" ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:PhoneNumber a owl:Class ;
    rdfs:label "PhoneNumber"@en ;
    rdfs:comment "A phone number" ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:Text a owl:Class ;
    rdfs:label "Text"@en ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:TimeZone a owl:Class ;
    rdfs:label "TimeZone"@en ;
    rdfs:comment "TZ timezone from the https://www.iana.org/time-zones. Timezone names never contain the space character but may contain an underscore. Refer to http://en.wikipedia.org/wiki/List_of_tz_zones for a list of valid values." ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:agency_email a gtfs:Field ;
    rdfs:label "agency_email" ;
    gtfs:example "info@citytransit.com" ;
    gtfs:isPresence gtfs:Optional ;
    gtfs:isType gtfs:Email ;
    rdfs:comment "Email address actively monitored by the agency’s customer service department. This email address should be a direct contact point where transit riders can reach a customer service representative at the agency." .

gtfs:agency_fare_url a gtfs:Field ;
    rdfs:label "agency_fare_url" ;
    gtfs:example "http://www.citytransit.com/fares" ;
    gtfs:isPresence gtfs:Optional ;
    gtfs:isType gtfs:URL ;
    rdfs:comment "URL of a web page that allows a rider to purchase tickets or other fare instruments for that agency online." .

gtfs:agency_id a gtfs:Field ;
    rdfs:label "agency_id" ;
    gtfs:additionalInfo "Required when the dataset contains data for multiple transit agencies. Recommended otherwise." ;
    gtfs:example "123" ;
    gtfs:isPresence gtfs:ConditionallyRequired ;
    gtfs:isType gtfs:ID ;
    rdfs:comment "Identifies a transit brand which is often synonymous with a transit agency. Note that in some cases, such as when a single agency operates multiple separate services, agencies and brands are distinct. This document uses the term \"agency\" in place of \"brand\". A dataset may contain data from multiple agencies." .

gtfs:agency_lang a gtfs:Field ;
    rdfs:label "agency_lang" ;
    gtfs:example "en" ;
    gtfs:isPresence gtfs:Optional ;
    gtfs:isType gtfs:LanguageCode ;
    rdfs:comment "Primary language used by this transit agency. Should be provided to help GTFS consumers choose capitalization rules and other language-specific settings for the dataset." .

gtfs:agency_name a gtfs:Field ;
    rdfs:label "agency_name" ;
    gtfs:example "City Transit" ;
    gtfs:isPresence gtfs:Required ;
    gtfs:isType gtfs:Text ;
    rdfs:comment "Full name of the transit agency." .

gtfs:agency_phone a gtfs:Field ;
    rdfs:label "agency_phone" ;
    gtfs:example "503-238-RIDE" ;
    gtfs:isPresence gtfs:Optional ;
    gtfs:isType gtfs:PhoneNumber ;
    rdfs:comment "A voice telephone number for the specified agency. This field is a string value that presents the telephone number as typical for the agency's service area. It may contain punctuation marks to group the digits of the number. Dialable text is permitted, but the field must not contain any other descriptive text." .

gtfs:agency_timezone a gtfs:Field ;
    rdfs:label "agency_timezone" ;
    gtfs:example "America/New_York" ;
    gtfs:isPresence gtfs:Required ;
    gtfs:isType gtfs:TimeZone ;
    rdfs:comment "Timezone where the transit agency is located. If multiple agencies are specified in the dataset, each must have the same agency_timezone." .

gtfs:agency_url a gtfs:Field ;
    rdfs:label "agency_url" ;
    gtfs:example "http://www.citytransit.com" ;
    gtfs:isPresence gtfs:Required ;
    gtfs:isType gtfs:URL ;
    rdfs:comment "URL of the transit agency." .

<http://gtfs.org/route_type/0> rdfs:label "0" ;
    rdfs:comment "Tram, Streetcar, Light rail. Any light rail or street level system within a metropolitan area." .

<http://gtfs.org/route_type/1> rdfs:label "1" ;
    rdfs:comment "Subway, Metro. Any underground rail system within a metropolitan area." .

<http://gtfs.org/route_type/2> rdfs:label "2" ;
    rdfs:comment "Rail. Used for intercity or long-distance travel." .

<http://gtfs.org/route_type/3> rdfs:label "3" ;
    rdfs:comment "Bus. Used for short- and long-distance bus routes." .

<http://gtfs.org/route_type/4> rdfs:label "4" ;
    rdfs:comment "Ferry. Used for short- and long-distance boat service." .

<http://gtfs.org/route_type/5> rdfs:label "5" ;
    rdfs:comment "Cable car. Used for street-level cable cars where the cable runs beneath the car." .

<http://gtfs.org/route_type/6> rdfs:label "6" ;
    rdfs:comment "Gondola, Suspended cable car. Typically used for aerial cable cars where the car is suspended from the cable." .

<http://gtfs.org/route_type/7> rdfs:label "7" ;
    rdfs:comment "Funicular. Any rail system designed for steep inclines." .

schema1:Enumeration a owl:Class ;
    rdfs:label "Enumeration"@en ;
    rdfs:subClassOf schema1:Intangible .

gtfs:URL a owl:Class ;
    rdfs:label "URL"@en ;
    rdfs:comment "A fully qualified URL that includes http:// or https://, and any special characters in the URL must be correctly escaped. See the following http://www.w3.org/Addressing/URL/4_URI_Recommentations.html for a description of how to create fully qualified URL values." ;
    rdfs:subClassOf gtfs:FieldType .

gtfs:File a owl:Class ;
    rdfs:label "File"@en ;
    rdfs:comment "A File refers to a single file within the GTFS feed" ;
    rdfs:subClassOf schema1:Intangible .

gtfs:Optional a owl:Class ;
    rdfs:label "Optional"@en ;
    rdfs:comment "The field or file may be omitted from the dataset." ;
    rdfs:subClassOf gtfs:PresenceCondition .

gtfs:Required a owl:Class ;
    rdfs:label "Required"@en ;
    rdfs:comment "The field or file must be included in the dataset and contain a valid value for each record." ;
    rdfs:subClassOf gtfs:PresenceCondition .

gtfs:PresenceCondition a owl:Class ;
    rdfs:label "PresenceCondition"@en ;
    rdfs:comment "The presence conditions applicable to  Files and Fields" ;
    rdfs:subClassOf schema1:Enumeration .

schema1:Intangible a owl:Class ;
    rdfs:label "Intangible"@en .

gtfs:Field a owl:Class ;
    rdfs:label "Field"@en ;
    rdfs:comment "A field refers to a single item of data or a column header within a GTFS file" ;
    rdfs:subClassOf schema1:Intangible .

gtfs:FieldType a owl:Class ;
    rdfs:label "FieldType"@en ;
    rdfs:subClassOf schema1:Intangible .

