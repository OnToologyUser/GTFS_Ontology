    <rdfs:subClassOf rdf:resource="http://schema.org/Intangible"/>
    <rdfs:label xml:lang="en">FareProdcut</rdfs:label>
  </owl:Class>
  <gtfs:ContinuousDropOffEnum rdf:about="http://gtfs.org/continuous_drop_off_1">
    <rdfs:comment>No continuous stopping drop off</rdfs:comment>
    <rdfs:label>continuous_drop_off:1</rdfs:label>
