  <gtfs:File rdf:about="http://gtfs.org/stop_areas.txt">
    <gtfs:hasField rdf:resource="http://gtfs.org/stop_id"/>
    <gtfs:hasField>
      <gtfs:Field rdf:about="http://gtfs.org/area_id">
        <rdfs:label>area_id</rdfs:label>
        <gtfs:additionalInfo>Foreign ID referencing `areas.area_id`</gtfs:additionalInfo>
        <gtfs:additionalInfo>Unique ID</gtfs:additionalInfo>
        <gtfs:isPresence rdf:resource="http://gtfs.org/Required"/>
        <rdfs:comment>Identifies an area to which one or multiple `stop_id`s belong. The same `stop_id` may be defined in many `area_id`s.</rdfs:comment>
        <rdfs:comment>Identifies an area. Must be unique in areas.txt.</rdfs:comment>
        <gtfs:isType rdf:resource="http://gtfs.org/ID"/>
      </gtfs:Field>
    </gtfs:hasField>
    <gtfs:isPresence rdf:resource="http://gtfs.org/Optional"/>
