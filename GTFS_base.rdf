        <gtfs:isType rdf:resource="http://gtfs.org/Text"/>
        <gtfs:isPresence rdf:resource="http://gtfs.org/Optional"/>
        <gtfs:additionalInfo>Field Type: Text</gtfs:additionalInfo>
        <rdfs:comment>The name of the area as displayed to the rider.</rdfs:comment>
      </gtfs:Field>
