    <gtfs:hasField rdf:resource="http://gtfs.org/area_id"/>
    <rdfs:comment>Area grouping of locations.</rdfs:comment>
    <gtfs:isPresence rdf:resource="http://gtfs.org/Optional"/>
    <rdfs:label>areas.txt</rdfs:label>
  </gtfs:File>
